<div class="col-md-12">

  <h2 class="text-center"> 我收藏的文章 </h2>

  <table class="table">
    <thead>
      <tr>
        <th> 用户名 </th>
        <th> 文章 </th>
      </tr>
    </thead>

    <tbody>
      <% @posts.each do |post| %>
        <tr>
          <td> <%= post.user.display_name %> </td>
          <td> <%= post.content %> </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
